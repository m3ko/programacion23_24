package Arrai;

import java.util.Scanner;

public class seis6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner scan = new Scanner(System.in);

		String[] palabras = new String[5];
		System.out.print("Escribe 5 palabras");
		for (int i = 0; i < 5; i++) {

			palabras[i] = scan.next();
		}
		System.out.println("Escribe un nombre para confirmar que esta en el Array");
		String aux = scan.next();
		for (int i = 0; i < 5; i++) {
			if (palabras[i].equals(aux)) {

				boolean x = true;
				System.out.println(x);
			} else {

				boolean x = false;
				System.out.println(x);
			}
		}
		scan.close();
	}
}
